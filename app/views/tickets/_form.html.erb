<%= form_for [@project, @ticket] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>>
    <% number = 0 %>
      <%= f.fields_for :assets do |asset| %>
        <p>
          <%= asset.label :asset, "File ##{number += 1}" %>
          <%= asset.file_field :asset %>
        </p>
      <% end %>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <%= f.submit %>
<% end %>

